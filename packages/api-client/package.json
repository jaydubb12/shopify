{
  "name": "@vue-storefront/shopify-api",
  "version": "1.0.8",
  "sideEffects": false,
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "server/index.d.ts",
  "server": "server/index.js",
  "scripts": {
    "build": "rimraf lib && rimraf server && rollup -c && yarn ts-coverage",
    "dev": "rollup -c -w",
    "test": "jest",
    "prepublish": "yarn build",
    "ts-coverage": "typescript-coverage-report"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.30.0",
    "@vue-storefront/core": "~2.5.0",
    "@vue-storefront/shopify-apollo": "^1.0.0",
    "graphql": "^14.5.8",
    "shopify-buy": "^2.11.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.1",
    "@types/shopify-buy": "^2.10.5",
    "jest-transform-graphql": "^2.1.0",
    "rollup-plugin-graphql": "^0.1.0"
  },
  "files": [
    "lib/**/*",
    "server/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typeCoverage": {
    "atLeast": 87.27,
    "is": 99,
    "cache": false,
    "debug": false,
    "detail": false,
    "ignoreCatch": true,
    "ignoreFiles": [
      "__tests__/**",
      "node_modules/**",
      "lib/**"
    ],
    "strict": false,
    "suppressError": false,
    "update": true,
    "ignoreUnread": true,
    "ignoreNested": true,
    "ignoreAsAssertion": true,
    "ignoreTypeAssertion": true,
    "ignoreNonNullAssertion": true,
    "showRelativePath": true,
    "historyFile": "typecoverage.json"
  }
}
